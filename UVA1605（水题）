//注意看清题目再答，不然很浪费时间。
#include<iostream>
#include<stdio.h>

using namespace std;
int main()
{
    int n;
    char c[52]={"QWERTYUIOPASDFGHJKLZXCVBNMzxcvbnmasdghjklqwertyuiop"};
    while(scanf("%d",&n)==1)
    {
        cout<<"2"<<" "<<n<<" "<<n<<endl;
        for(int j=0;j<n;j++)
        {
            for(int i=0;i<n;i++)
            {
                cout<<c[j];
            }
            cout<<endl;
        }
        cout<<endl;
        for(int j=0;j<n;j++)
        {
            for(int i=0;i<n;i++)
            {
                cout<<c[i];
            }
            cout<<endl;
        }
        cout<<endl;
    }
}
